# AI Provider API Keys
# Uncomment and add your API keys as needed

# OpenAI API key for AI capabilities
VITE_OPENAI_API_KEY=your_openai_api_key

# Anthropic API key (optional, for Claude models)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key

# Stripe public key (from Stripe Dashboard)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# Stripe Price IDs for different plans (from Stripe Dashboard)
VITE_STRIPE_PRICE_ID_BASIC=price_your_basic_price_id
VITE_STRIPE_PRICE_ID_PRO=price_your_pro_price_id
VITE_STRIPE_PRICE_ID_ENTERPRISE=price_your_enterprise_price_id

# API Base URL for serverless functions
VITE_API_BASE_URL=http://localhost:3000/api

# App URL for Stripe redirects
VITE_APP_URL=http://localhost:5173

# Stripe Webhook Signing Secret (from `stripe listen` or Dashboard webhook)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_signing_secret

# Server-side secret key (configure in deployment platform, not needed client-side build)
# STRIPE_SECRET_KEY=sk_test_your_server_secret_key

# Upstash Redis (for rate limiting)
# VITE_UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url
# VITE_UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token

# Vercel Blob Storage 
# VITE_BLOB_READ_WRITE_TOKEN=your_blob_read_write_token
